<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title | XavSpace</title>
    @Html.Partial("LayoutPartials/_head")
    @RenderSection("head", required: false)
</head>
<body style="background-image: url('../../Content/images/IMG_3265.jpg');background-repeat: no-repeat; background-size:cover ">
    @Html.Partial("LayoutPartials/_navbar")
    <div class="container body-content">
        <br>
        <br>
        @RenderBody()
        <hr />
        @Html.Partial("LayoutPartials/_footer")
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/ta")
    @RenderSection("scripts", required: false)
</body>
</html>
